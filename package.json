{
  "name": "boondocker",
  "version": "0.0.0",
  "description": "App to show all available free and low cost campgrounds",
  "main": "index.js",
  "scripts": {},
  "repository": {
    "type": "git",
    "url": "git://github.com/thlorenz/boondocker.git"
  },
  "homepage": "https://github.com/thlorenz/boondocker",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "env": {
            "development": {
              "plugins": [
                [
                  "react-transform",
                  {
                    "transforms": [
                      {
                        "transform": "livereactload/babel-transform",
                        "imports": [
                          "react"
                        ]
                      }
                    ]
                  }
                ]
              ]
            }
          }
        }
      ]
    ]
  },
  "dependencies": {
    "domready": "~1.0.8",
    "react": "~15.1.0",
    "react-dom": "~15.1.0",
    "redux": "~3.5.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "~2.0.2",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babelify": "~7.3.0",
    "browserify": "~13.0.1",
    "cheerio": "~0.20.0",
    "livereactload": "~2.2.4",
    "react-tools": "~0.10.0",
    "react-transform": "0.0.3",
    "uglifyjs": "~2.4.10",
    "watchify": "~3.7.0"
  },
  "keywords": [],
  "author": {
    "name": "Thorsten Lorenz",
    "email": "thlorenz@gmx.de",
    "url": "http://thlorenz.com"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/thlorenz/boondocker/blob/master/LICENSE"
  },
  "engine": {
    "node": ">=0.8"
  }
}
